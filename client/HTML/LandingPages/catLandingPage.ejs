<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Cat website</title>
    <link rel="stylesheet" href="../../css/LandingPages/catLandingPage.css" />
    <link rel="stylesheet" href="https://fonts.googleapis.com/icon?family=Material+Icons+Sharp" />
    <link rel="stylesheet" href="https://fonts.googleapis.com/icon?family=Material+Icons+Outlined" />  
    <link
      rel="stylesheet"
      href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.3.0/css/all.min.css"
      integrity="sha512-SzlrxWUlpfuzQ+pcUCosxcglQRNAq/DZjVsC0lE40xsADsfeQoEypE+enwcOiGjk/bSuGGKHEyjSoQ1zVisanQ=="
      crossorigin="anonymous"
      referrerpolicy="no-referrer"
    />
  </head>
  <body>
    <%- include("../components/navbar", {notlogin: true}); %>
    

    <section class="home" id="home">
      <div class="content">
        <h3>Find your <span> Perfect Cat</span> on our website</h3>
        <a href="#" class="glow-on-hover">Shop Now</a>
      </div>
      <img src="../../img/catLandingPage/bottom_wave.png" alt="" class="wave" />
    </section>

    <section class="about"></section>

    <section id="Shop-Cat" class="Shop-Cat">
      <h3 class="detail">
        <span class="word1">Our</span> <span class="word2"> Popular</span>
        <span class="word3"> Breeds</span>
      </h3>

      <div class="products">
        <div class="product">
          <h3>Persian Cat</h3>
          <div class="card">
            <div class="front">
              <img
                src="../../img/catLandingPage/Cat_Breeds/persian.png"
                alt=""
              />
            </div>
            <div class="back">
              <p class="info">8 Weeks Old</p>
              <p class="info">Male|Female</p>
              <p class="info cost">Rs. 20000 <span>Rs. 35000.00</span></p>
              <a href="" class="pet"><span>Shop Now</span></a>
            </div>
          </div>
        </div>

        <div class="product">
          <h3>Ragdoll</h3>
          <div class="card">
            <div class="front">
              <img
                src="../../img/catLandingPage/Cat_Breeds/Ragdoll.png"
                alt=""
              />
            </div>
            <div class="back">
              <p class="info">8 Weeks Old</p>
              <p class="info">Male|Female</p>
              <p class="info cost">Rs. 25000 <span>Rs. 35000.00</span></p>
              <a href="" class="pet"><span>Shop Now</span></a>
            </div>
          </div>
        </div>

        <div class="product">
          <h3>British Shorthair</h3>
          <div class="card">
            <div class="front">
              <img
                src="../../img/catLandingPage/Cat_Breeds/british shorthair.png"
                alt=""
              />
            </div>
            <div class="back">
              <p class="info">8 Weeks Old</p>
              <p class="info">Male|Female</p>
              <p class="info cost">Rs. 40000 <span>Rs. 60000.00</span></p>
              <a href="" class="pet"><span>Shop Now</span></a>
            </div>
          </div>
        </div>

        <div class="product">
          <h3>Scottish Fold</h3>
          <div class="card">
            <div class="front">
              <img
                src="../../img/catLandingPage/Cat_Breeds/scottish-fold.png"
                alt=""
              />
            </div>
            <div class="back">
              <p class="info">8 Weeks Old</p>
              <p class="info">Male|Female</p>
              <p class="info cost">Rs. 15000 <span>Rs. 18000.00</span></p>
              <a href="" class="pet"><span>Shop Now</span></a>
            </div>
          </div>
        </div>
        <div class="product exploreMore">
          <div class="explore"><a href="#" class="expBtn">Find More</a></div>
        </div>

        <a class="prev" onclick="plusSlides(-1)">&#10094;</a>
        <a class="next" onclick="plusSlides(1)">&#10095;</a>
      </div>
      <div class="explore"><a href="#" class="expBtn">Explore More</a></div>
    </section>

    <section id="services" class="services">
      <h3 class="heading">Our Services</h3>
      <div class="services-list">
        <div class="service">
          <img
            src="../../img/catLandingPage/Cat_Breeds/cat-accessories.png"
            alt=""
          />
          <div class="content">
            <h3>Accesories</h3>
            <p>
              Here, You can find all best deals on Accesories related to pet
              including Toys,Harnesses
            </p>
            <a class="pet pet-service" href="#">Learn More</a>
          </div>
        </div>
        <div class="service">
          <img
            src="../../img/catLandingPage/Cat_Breeds/cat-care.png"
            alt=""
            height="250px"
            width="250px"
          />
          <h3>Pet Care</h3>
          <p>Here You can find all the best medicines to care your Pet</p>
          <a class="pet pet-service" href="#">Learn More</a>
        </div>
        <div class="service">
          <img src="../../img/catLandingPage/Cat_Breeds/cat-food.png" alt="" />
          <h3>Food</h3>
          <p>Feed Your Pet with Love , Affection and High Quality Food!</p>
          <a class="pet pet-service" href="#">Learn More</a>
        </div>
        <div class="service">
          <img
            src="../../img/catLandingPage/Cat_Breeds/pet-grooming.png"
            alt=""
          />
          <h3>Saloon</h3>
          <p>Every Pet deserves Celebrity Care</p>
          <a class="pet pet-service" href="#">Learn More</a>
        </div>
        <div class="service">
          <img
            src="../../img/catLandingPage/Cat_Breeds/cat-training.png"
            alt=""
          />
          <h3>Training</h3>
          <p>Give You Pet the best training for desired behaviours</p>
          <a class="pet pet-service" href="#">Learn More</a>
        </div>
      </div>
    </section>

    <section class="stats">
      <div class="Delivered">
        <span class="count">999</span>
        <p>Cats Delivered</p>
      </div>

      <div class="feedback">
        <span class="count">99%</span>
        <p>Positive Customers</p>
      </div>

      <div class="happy-families">
        <span class="count">500</span>
        <p>Happy Families</p>
      </div>
    </section>

    <%- include("../components/footer"); %>


    <!-- <a href="" class="pet"><span>Shop Now</span></a> -->
    <!-- <button type="button" class="pet"><span>Shop Now</span></button> -->
  </body>
  <script src="../../JavaScript/LandingPages/petsLandingPage.js"></script>
</html>

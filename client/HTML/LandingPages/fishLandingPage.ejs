      <div class="pet-services">
        <p>Services Offered</p>
        <a href="#" class="pets">Accessories</a>
        <a href="#" class="pets">Training</a>
        <a href="#" class="pets">Pet Care</a>
        <a href="#" class="pets">Spa & Grooming</a>
      </div>

      <div class="social">
        <p>Lets Stay Connected</p>
        <div class="input">
          <input type="text" placeholder="Your Mail" />
          <button type="button">Submit</button>
        </div>
        <p class="follow">Follow Us</p>
        <a href="#"><i class="fa-brands fa-facebook facebook"></i></a>
        <a href="#"><i class="fa-brands fa-square-instagram instagram"></i></a>
        <a href="#"><i class="fa-brands fa-square-twitter twitter"></i></a>
        <a href="#"><i class="fa-brands fa-pinterest pinterest"></i></a>
      </div>
